   "source": [
    "x =  train_data[:,0]\n",
    "y =  train_data[:,1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "m =  0.17989290468639363\n",
      "c =  0.00988579231737737\n"
     ]
    }
   ],
   "source": [
    "m = 0\n",
    "c = 0 \n",
    "\n",
    "L = 0.0001 #Learning rate \n",
    "iterations = 1000\n",
    "\n",
    "n = float(len(x))\n",
    "\n",
    "for i in range(iterations):\n",
    "    y_pred = m*x + c\n",
    "    D_m = (-1/n) * sum(x * (y - y_pred))\n",
    "    D_c = (-2/n) * sum(y - y_pred)\n",
    "    m = m - L * D_m\n",
    "    c = c - L * D_c\n",
    "    \n",
    "print(\"m = \", m)\n",
    "print(\"c = \", c)"
   ]
